" Pretty defaults
syntax enable
colorscheme solarized
set background=dark
set guifont=Meslo\ LG\ M\ DZ:h13
set statusline=%F%m%r%h%w\ [FORMAT=%{&ff}]\ [TYPE=%Y]\ [ASCII=\%03.3b]\ [HEX=\%02.2B]\ [POS=%04l,%04v][%p%%]\ [LEN=%L] 
set laststatus=2
set guioptions-=T
set list!
set listchars=tab:▸\ ,eol:¬
set nu
